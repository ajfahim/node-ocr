{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "functions": {
    "api/ocr/base64": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/ocr/upload": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
